{
  "name": "naran/geo-ip",
  "description": "Get country of client by IP address.",
  "type": "wordpress-plugin",
  "minimum-stability": "stable",
  "require": {
    "geoip2/geoip2": "^2.11"
  },
  "autoload": {
    "files": [
      "includes/functions.php"
    ],
    "classmap": [
      "includes"
    ]
  },
  "scripts": {
    "daa": "@composer dump-autoload --classmap-authoritative",
    "make-pot": "wp i18n make-pot . ./languages/naran-geo-ip.pot --domain=ngip"
  },
  "scripts-descriptions": {
    "daa": "Run `composer dump-autoload --classmap-authoritative`.",
    "make-pot": "Create .pot file."
  },
  "version": "1.0.0"
}
